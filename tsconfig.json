{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*":[
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@icons/*":[
        "src/icons/*"
      ],
      "@ui/*":[
        "src/ui/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@data/*": [
        "src/data/*"
      ]
    }
  },
}
